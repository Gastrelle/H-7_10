#include <stdio.h>
#include <locale.h>
int main() {
    setlocale(LC_CTYPE, "");
    int date;

    printf("Введите дату в формате ddmm: ");
    scanf("%d", &date);
    
    int day = date / 100;    
    int month = date % 100; 
    
    if (day < 1 || day > 31 || month < 1 || month > 12) {
        printf("Некорректная дата.\n");
        return 1;
    }
    
    printf("Дата %02d.%02d - ", day, month);
    
    switch(month) {
        case 1:
            if (day <= 20) printf("Козерог\n");
            else printf("Водолей\n");
            break;
        case 2:
            if (day > 29) printf("Не существует\n");
            break;
            if (day <= 19) printf("Водолей\n");
            else printf("Рыбы\n");
            break;
        case 3: 
            if (day <= 20) printf("Рыбы\n");
            else printf("Овен\n");
            break;
        case 4:
            if (day <= 20) printf("Овен\n");
            else printf("Телец\n");
            break;
        case 5:
            if (day <= 21) printf("Телец\n");
            else printf("Близнецы\n");
            break;
        case 6:
            if (day <= 21) printf("Близнецы\n");
            else printf("Рак\n");
            break;
        case 7:
            if (day <= 22) printf("Рак\n");
            else printf("Лев\n");
            break;
        case 8:
            if (day <= 23) printf("Лев\n");
            else printf("Дева\n");
            break;
        case 9:
            if (day <= 23) printf("Дева\n");
            else printf("Весы\n");
            break;
        case 10:
            if (day <= 23) printf("Весы\n");
            else printf("Скорпион\n");
            break;
        case 11:
            if (day <= 22) printf("Скорпион\n");
            else printf("Стрелец\n");
            break;
        case 12:
            if (day <= 21) printf("Стрелец\n");
            else printf("Козерог\n");
            break;
    }
    
    return 0;
}
